<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipChildren="false">

    <ImageView
        android:id="@+id/cloud"
        android:layout_width="245dp"
        android:layout_height="260dp"
        android:src="@drawable/intro_clouds_2"
        android:layout_centerInParent="true"/>

    <ImageView
        android:id="@+id/centerImage"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:layout_centerInParent="true"
        app:srcCompat="@drawable/intro_merchant" />


    <ImageView
        android:id="@+id/image1"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/intro_large_line"
        android:layout_above="@id/centerImage"
        android:layout_marginBottom="30dp"/>

    <ImageView
        android:id="@+id/image2"
        android:layout_width="10dp"
        android:layout_height="10dp"
        app:srcCompat="@drawable/ic_intro_cross"
        android:layout_toRightOf="@+id/image1"
        android:layout_above="@+id/centerImage"
        android:layout_marginBottom="30dp"
        android:layout_marginLeft="30dp"/>

    <ImageView
        android:id="@+id/image3"
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:srcCompat="@drawable/intro_large_line"
        android:layout_above="@id/centerImage"
        android:layout_toRightOf="@+id/image2"
        android:layout_marginLeft="20dp"/>


    <ImageView
        android:id="@+id/image4"
        android:layout_width="10dp"
        android:layout_height="10dp"
        app:srcCompat="@drawable/ic_intro_circle"
        android:layout_above="@+id/centerImage"
        android:layout_toRightOf="@+id/image3"
        android:layout_marginLeft="10dp"/>


    <ImageView
        android:id="@+id/image5"
        android:layout_width="10dp"
        android:layout_height="10dp"
        app:srcCompat="@drawable/ic_intro_plus"
        android:layout_alignRight="@+id/centerImage"
        android:layout_below="@+id/image4"
        android:layout_marginTop="15dp"/>

    <ImageView
        android:id="@+id/image6"
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:srcCompat="@drawable/intro_large_line"
        android:layout_toRightOf="@+id/centerImage"
        android:layout_below="@+id/image5"
        android:layout_marginTop="20dp"/>

    <ImageView
        android:id="@+id/image7"
        android:layout_width="10dp"
        android:layout_height="10dp"
        app:srcCompat="@drawable/ic_intro_plus"
        android:layout_toRightOf="@+id/centerImage"
        android:layout_below="@+id/image6"
        android:layout_marginTop="20dp"/>

    <ImageView
        android:id="@+id/image8"
        android:layout_width="10dp"
        android:layout_height="10dp"
        app:srcCompat="@drawable/ic_intro_small_line"
        android:layout_toRightOf="@+id/centerImage"
        android:layout_below="@+id/image7"
        android:layout_marginTop="20dp"/>

    <ImageView
        android:id="@+id/image10"
        android:layout_width="10dp"
        android:layout_height="10dp"
        app:srcCompat="@drawable/ic_intro_small_line"
        android:layout_toRightOf="@+id/centerImage"
        android:layout_below="@+id/image7"
        android:layout_marginTop="20dp"/>

    <ImageView
        android:id="@+id/image9"
        android:layout_width="10dp"
        android:layout_height="10dp"
        app:srcCompat="@drawable/ic_intro_small_line"
        android:layout_above="@+id/centerImage"
        android:layout_toLeftOf="@+id/image1"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="40dp"/>

    <ImageView
        android:id="@+id/image11"
        android:layout_width="10dp"
        android:layout_height="10dp"
        app:srcCompat="@drawable/ic_intro_circle"
        android:layout_above="@+id/centerImage"
        android:layout_toLeftOf="@+id/image9"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="10dp"/>

    <ImageView
        android:id="@+id/image12"
        android:layout_width="10dp"
        android:layout_height="10dp"
        app:srcCompat="@drawable/ic_intro_cross"
        android:layout_toLeftOf="@+id/centerImage"
        android:layout_alignTop="@id/centerImage"
        android:layout_marginTop="20dp"/>

    <ImageView
        android:id="@+id/image13"
        android:layout_width="10dp"
        android:layout_height="10dp"
        app:srcCompat="@drawable/ic_intro_small_line"
        android:layout_toLeftOf="@+id/centerImage"
        android:layout_below="@+id/image12"
        android:layout_marginTop="30dp"/>

    <ImageView
        android:id="@+id/image14"
        android:layout_width="10dp"
        android:layout_height="10dp"
        app:srcCompat="@drawable/ic_intro_plus"
        android:layout_toLeftOf="@id/centerImage"
        android:layout_below="@+id/image13"
        android:layout_marginTop="5dp"
        android:layout_marginRight="20dp"/>


    <ImageView
        android:id="@+id/image15"
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:srcCompat="@drawable/intro_large_line"
        android:layout_toLeftOf="@+id/centerImage"
        android:layout_below="@+id/image14"
        android:layout_marginRight="20dp"
        android:layout_marginTop="10dp"/>

    <ImageView
        android:id="@+id/image16"
        android:layout_width="10dp"
        android:layout_height="10dp"
        app:srcCompat="@drawable/ic_intro_cross"
        android:layout_toLeftOf="@+id/centerImage"
        android:layout_below="@+id/image15"
        android:layout_marginTop="10dp"
        android:layout_marginRight="15dp"/>

</RelativeLayout>